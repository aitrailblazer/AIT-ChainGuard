#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/constantinevassilev02/MyLocalDocuments/go-projects/AIT-ChainGuard/node_modules/cborg/node_modules:/Users/constantinevassilev02/MyLocalDocuments/go-projects/AIT-ChainGuard/node_modules:/Users/constantinevassilev02/MyLocalDocuments/go-projects/node_modules:/Users/constantinevassilev02/MyLocalDocuments/node_modules:/Users/constantinevassilev02/node_modules:/Users/node_modules:/node_modules:/Users/constantinevassilev02/MyLocalDocuments/go-projects/AIT-ChainGuard/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/constantinevassilev02/MyLocalDocuments/go-projects/AIT-ChainGuard/node_modules/cborg/node_modules:/Users/constantinevassilev02/MyLocalDocuments/go-projects/AIT-ChainGuard/node_modules:/Users/constantinevassilev02/MyLocalDocuments/go-projects/node_modules:/Users/constantinevassilev02/MyLocalDocuments/node_modules:/Users/constantinevassilev02/node_modules:/Users/node_modules:/node_modules:/Users/constantinevassilev02/MyLocalDocuments/go-projects/AIT-ChainGuard/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../cborg/cli.js" "$@"
else
  exec node  "$basedir/../cborg/cli.js" "$@"
fi
